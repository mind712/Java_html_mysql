#!/bin/bash
###########################################################
# Script        : Throttling_Count.sh
# Author        : Satyanarayan Nayak
# Description   : This is a sub script which has the details of previous day Throttling Error Count for particular server.
# User          : ****
###########################################################
y=2
dt=1
echo "Throttling details for the Host RTMPR041"
LOG_PATH=/var/sitelogs/osb_pr04*/osb_pr04*_BusServer01/logs
cd $LOG_PATH
rm throttle_error
grep "`date -d -1day '+%Y-%m-%d'`" main* > throttle_error
grep "`date -d -1day '+%d-%b-%Y'`" main* >> throttle_error
a=`grep "BEA-380100" throttle_error |wc -l`
x=$((a/y))
echo "Total Count BEA-380100 is $x"
b=`grep "BEA-380101" throttle_error |wc -l`
z=$((b/y))
echo "Total Count BEA-380101 is $z"
c=`grep "BEA-380102" throttle_error |wc -l`
t=$((c/y))
echo "Total Count BEA-380102 is $t"
d=`grep "BEA-380103" throttle_error |wc -l`
s=$((d/y))
echo "Total Count BEA-380103 is $s"







