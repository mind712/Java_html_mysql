#!/bin/bash
###########################################################
# Script        : Throttling_ARTM.sh
# Author        : Satyanarayan Nayak
# Description   : This is a main script which send email daily at 8:45 with previous day Throttling Error Count for all 6 ARTM Servers.
#                 Mail will be triggered to EIM,APPSupport And Middleware Team.
# User          : ****
###########################################################
cd /var/domain/osb_pr04*/script/Throttling
/var/domain/osb_pr04*/script/Throttling/Throttling_Count.sh > file.tmp
ssh server1 "sh /var/domain/osb_pr04*/script/Throttling/Throttling_Count.sh > /var/domain/osb_pr04a/script/Throttling/output_RTMPR041"
scp server1:/var/domain/osb_pr04*/script/Throttling/output_RTMPR041 .
cat output_RTMPR041 >> file.tmp
ssh server1 "sh /var/domain/osb_pr04*/script/Throttling/Throttling_Count.sh > /var/domain/osb_pr04a/script/Throttling/output_RTMPR042"
scp server1:/var/domain/osb_pr04*/script/Throttling/output_RTMPR042 .
cat output_RTMPR042 >> file.tmp
ssh server2 "sh /var/domain/osb_pr04*/script/Throttling/Throttling_Count.sh > /var/domain/osb_pr04a/script/Throttling/output_RTMPR043"
scp server2:/var/domain/osb_pr04*/script/Throttling/output_RTMPR043 .
cat output_RTMPR043 >> file.tmp
ssh server3 "sh /var/domain/osb_pr04*/script/Throttling/Throttling_Count.sh > /var/domain/osb_pr04b/script/Throttling/output_RTMPR044"
scp server3:/var/domain/osb_pr04*/script/Throttling/output_RTMPR044 .
cat output_RTMPR044 >> file.tmp
ssh server4 "sh /var/domain/osb_pr04*/script/Throttling/Throttling_Count.sh > /var/domain/osb_pr04b/script/Throttling/output_RTMPR045"
scp server4:/var/domain/osb_pr04*/script/Throttling/output_RTMPR045 .
cat output_RTMPR045 >> file.tmp
ssh server5 "sh /var/domain/osb_pr04*/script/Throttling/Throttling_Count.sh > /var/domain/osb_pr04b/script/Throttling/output_RTMPR046"
scp server5:/var/domain/osb_pr04*/script/Throttling/output_RTMPR046 .
cat output_RTMPR046 >> file.tmp
mailbody="Testmail via bash script"
echo "From:****@capitalone.com" > /tmp/mailtest
echo "To:****@capitalone.com" >> /tmp/mailtest
echo "Subject: Throttling details on `date -d -1day '+%d-%b-%Y'`" >> /tmp/mailtest
echo "Hi All," >> /tmp/mailtest
echo -e "\n Below are the Throttling Error Count Details on `date -d -1day '+%d-%b-%Y'`" >> /tmp/mailtest
echo -e "\n" >> /tmp/mailtest
cat file.tmp >> /tmp/mailtest
echo -e "\n BEA-380100 - Throttling error: Business service is throttled and rejected the message because the service reached its maximum concurrency limit and has no throttling queue to backlog messages. This is not applicable in our environment as we have configured the Queue depth of 20." >> /tmp/mailtest
echo -e "\n BEA-380101 - Throttling error: message expired in throttling queue for business service." >> /tmp/mailtest
echo -e "\n BEA-380102 - The message was sent to a throttled business service and was rejected by its throttling queue. The queue may be full and the message priority lesser than the messages already in the queue; or the queue was resized or the service renamed while the message was waiting in the queue." >> /tmp/mailtest
echo -e "\n BEA-380103 - An internal error occurred in the throttling sub-system." >> /tmp/mailtest
echo -e "\n" >> /tmp/mailtest
echo "Regards," >> /tmp/mailtest
echo "Support Team" >> /tmp/mailtest
cat /tmp/mailtest | /usr/sbin/sendmail -t

